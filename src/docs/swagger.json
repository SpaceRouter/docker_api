                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/forms.StackSearchResponse"
                        }
                    }
                }
            }
        },
        "/v1/stack/{id}": {
            "get": {
                "description": "Get all stack information from stack ID",
                "produces": [
                    "application/json"
                ],
                "summary": "Get stack by ID",
                "operationId": "get_stack_by_id",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
